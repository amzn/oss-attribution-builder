{
  "name": "amazon-oss-attribution-builder",
  "version": "0.9.0",
  "description": "A website to help you create attribution documents for product releases",
  "repository": {
    "type": "git",
    "url": "https://github.com/amzn/oss-attribution-builder"
  },
  "main": "server/app.js",
  "scripts": {
    "build": "tsc && webpack && npm run copy-static",
    "copy-static": "mkdir -p build/server && cp -r config data build && cp -r assets build/server",
    "dev": "export NODE_ENV=development CONFIG_NAME=dev && npm run copy-static && concurrently -k \"docker-compose -f docker-compose.dev.yml up\" \"tsc -w\" \"nodemon -d 1 -w build build/server/localserver.js\" \"webpack-dev-server\"",
    "start": "export NODE_ENV=production && npm run build && node ./build/server/localserver.js",
    "test": "npm run build && jasmine 'build/server/**/*.spec.js'",
    "test-ui": "trap 'docker-compose -f docker-compose.selenium.yml stop' INT HUP EXIT; docker-compose -f docker-compose.selenium.yml up -d --build && tsc && jasmine 'build/selenium/*.spec.js'",
    "lint": "tslint -c tslint.json -p . --fix && tslint -c tslint.json -p browser --fix"
  },
  "author": "Jacob Peddicord <peddicor@amazon.com>",
  "license": "Apache-2.0",
  "engines": {
    "node": "^6.9.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.4",
    "@types/bootstrap": "^3.3.35",
    "@types/compression": "0.0.33",
    "@types/history": "^4.6.0",
    "@types/jasmine": "^2.6.0",
    "@types/jquery": "^3.2.10",
    "@types/mockery": "^1.4.29",
    "@types/mz": "0.0.31",
    "@types/node": "^6.0.72",
    "@types/node-fetch": "^1.6.7",
    "@types/passport": "^0.3.3",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-router-dom": "^4.0.7",
    "@types/react-select": "^1.0.59",
    "@types/selenium-webdriver": "^3.0.3",
    "@types/webpack": "^3.0.10",
    "@types/winston": "2.3.4",
    "bootstrap": "^4.0.0-beta.2",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "font-awesome": "^4.7.0",
    "history": "^4.6.3",
    "jasmine": "^2.8.0",
    "jquery": "^3.2.1",
    "mockery": "^2.1.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "pg-monitor": "^0.8.2",
    "popper.js": "^1.12.9",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "react-select": "^1.1.0",
    "redux": "^3.7.2",
    "sass-loader": "^6.0.6",
    "selenium-webdriver": "^3.0.1",
    "ts-loader": "^2.3.2",
    "tslint": "^5.5.0",
    "tslint-language-service": "^0.9.7",
    "typescript": "~2.6.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.6.1",
    "core-js": "^2.4.1",
    "express": "^4.13.3",
    "immutable": "^3.7.6",
    "moment": "^2.17.0",
    "mz": "^2.4.0",
    "node-fetch": "^1.6.3",
    "passport": "^0.4.0",
    "pg-promise": "^6.3.8",
    "source-map-support": "^0.4.0",
    "whatwg-fetch": "^2.0.3",
    "winston": "^2.2.0",
    "winston-daily-rotate-file": "^1.4.3",
    "wordwrap": "^1.0.0"
  },
  "optionalDependencies": {
    "cookie-parser": "^1.4.3",
    "passport-cookie": "^1.0.4",
    "passport-http": "^0.3.0"
  }
}
